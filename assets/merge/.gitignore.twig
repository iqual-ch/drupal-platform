# These are some examples of commonly ignored file patterns.
# You should customize this list as applicable to your project.
# Learn more about .gitignore:
#     https://www.atlassian.com/git/tutorials/saving-changes/gitignore

# Node artifact files
node_modules/
dist/

# Compiled Java class files
*.class

# Compiled Python bytecode
*.py[cod]

# Log files
*.log

# Package files
*.jar

# Maven
target/
dist/

# JetBrains IDE
.idea/

# Unit test reports
TEST*.xml

# Generated by MacOS
.DS_Store

# Generated by Windows
Thumbs.db

# Applications
*.app
*.exe
*.war

# Large media files
*.mp4
*.tiff
*.avi
*.flv
*.mov
*.wmv

# App specific
app/vendor
app/private
app/public/core
app/public/libraries
app/public/modules/contrib
app/public/modules/custom/*
app/public/themes/contrib
app/public/themes/custom/*
app/public/profiles/contrib
app/public/profiles/custom
app/public/sites/simpletest
app/public/sites/default/files/*
app/public/sites/default/settings.local.php
app/public/sites/default/services.local.yml
db.session.sql

# Custom themes and modules
!app/public/themes/custom/iq_custom
!app/public/modules/custom/{{ name|replace({'-': '_'}) }}
!app/public/modules/custom/{{ name|replace({'-': '_'}) }}_*

# Assets
app/resources/*.sql
app/resources/*.sql.gz
app/resources/*.tar
app/resources/*.tar.gz
!app/public/sites/default/files/fontyourface

# Tooling
.composer
.console
.drush

# Backups
app/backups/

# Ignore secrets
.env.secret*

# Ignore local ephemeral variables
.env.local

# Ignore maintenance file
maintenance_*.html
